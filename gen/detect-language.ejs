#!/bin/bash

<% for ( let lang of lbypop ) { -%>
<% if ( lang.entrypoint ) { -%>
if [ -f <%= lang.entrypoint %> ]; then echo '<%= lang.name %>'; exit 0; fi
<% } -%>
<% } -%>

<% for ( let lang of lbypop ) { -%>
<% if ( lang.extensions ) { -%>
<% for ( let ex of lang.extensions ) { -%>
if [ -n "$(find . -name '<%= ex %>')" ]; then echo '<%= lang.name %>'; exit 0; fi
<% } -%>
<% } -%>
<% } -%>