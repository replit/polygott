{
	"definitions": {
		"command": {
			"$id": "#/definitions/command",
			"type": "array",
			"title": "The Command Schema",
			"items": {
				"$id": "#/definitions/command/command/items",
				"type": "string",
				"title": "The Items Schema",
				"default": "",
				"examples": [
					"pyls",
					"-v"
				],
				"pattern": "^(.*)$",
				"additionalProperties": false
			}
		},
		"test": {
			"$id": "#/definitions/test",
			"type": "object",
			"title": "The Test Schema",
			"required": [
				"code",
				"output"
			],
			"properties": {
				"code": {
					"$id": "#/properties/tests/properties/hello/properties/code",
					"type": "string",
					"title": "The Code Schema",
					"default": "",
					"examples": [
						"print \"hello\""
					]
				},
				"output": {
					"$id": "#/properties/tests/properties/hello/properties/output",
					"type": "string",
					"title": "The Output Schema",
					"default": "",
					"examples": [
						"hello\n"
					]
				},
				"skip": {
					"type": "boolean",
					"default": false
				},
				"additionalProperties": false
			}
		},
		"exec": {
			"$id": "#/definitions/exec",
			"type": "object",
			"title": "The Exec Schema",
			"required": [
				"command"
			],
			"properties": {
				"command": {
					"$ref": "#/definitions/command"
				},
				"pty": {
					"$id": "#/properties/run/properties/pty",
					"type": "boolean",
					"title": "The Pty Schema",
					"default": false,
					"examples": [
						true
					]
				}
			},
			"additionalProperties": false
		}
	},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://repl.it/language.json",
	"type": "object",
	"title": "Polygott Language",
	"required": [
		"entrypoint",
		"extensions",
		"name",
		"run"
	],
	"properties": {
		"aptKeys": {
			"type": "array",
			"items": {
				"$id": "#/properties/aptRepos/items",
				"type": "string",
				"title": "The Items Schema",
				"default": ""
			}
		},
		"aptRepos": {
			"type": "array",
			"items": {
				"$id": "#/properties/aptRepos/items",
				"type": "string",
				"title": "The Items Schema",
				"default": ""
			}
		},
		"entrypoint": {
			"type": "string",
			"title": "The Entrypoint Schema",
			"default": "",
			"examples": [
				"main.py"
			],
			"pattern": "^(.*)$"
		},
		"extensions": {
			"type": "array",
			"title": "The Extensions Schema",
			"items": {
				"$id": "#/properties/extensions/items",
				"type": "string",
				"default": "",
				"examples": [
					"py"
				],
				"pattern": "^(.*)$"
			}
		},
		"languageServer": {
			"type": "object",
			"title": "The Languageserver Schema",
			"required": [
				"command"
			],
			"properties": {
				"command": {"$ref": "#/definitions/command"}
			}
		},
		"name": {
			"type": "string",
			"title": "The Name Schema",
			"default": "",
			"examples": [
				"python"
			],
			"pattern": "^([a-z_]*[0-9.]*)$"
		},
		"packages": {
			"type": "array",
			"title": "The Packages Schema",
			"items": {
				"$id": "#/properties/packages/items",
				"type": "string",
				"title": "The Items Schema",
				"default": "",
				"examples": [
					"python-pip",
					"python-wheel",
					"libtk8.6",
					"python-dev",
					"libevent-dev",
					"gcc"
				],
				"pattern": "^([a-z0-9.-]+)$"
			}
		},
		"popularity": {
			"title": "Populartiy",
			"type": "integer" 
		},
		"run": {
			"$ref": "#/definitions/exec"
		},
		"compile": {
			"type": "object",
			"required": [
				"command"
			],
			"properties": {
				"command": {
					"$ref": "#/definitions/command"
				},
				"pty": {
					"type": "boolean",
					"default": false
				},
				"onlyMain": {
					"type": "boolean",
					"default": false
				}
			},
			"additionalProperties": false
		},
		"versionCommand": {
			"$ref": "#/definitions/command"
		},
		"setup": {
			"type": "array",
			"title": "The Setup Schema",
			"items": {
				"type": "string",
				"default": "",
				"examples": [
					"wget https://storage.googleapis.com/container-bins/stderred_1.0_amd64.deb && dpkg -i stderred_1.0_amd64.deb && rm stderred_1.0_amd64.deb",
					"pip install -U setuptools",
					"pip install -U configparser",
					"pip install --no-cache-dir pipreqs-amasad==0.4.10 python-language-server==0.21.2 jedi==0.12.1 pyflakes==2.0.0 rope==0.11.0 yapf==0.25.0 pycodestyle==2.4.0 mccabe==0.6.1 nltk numpy scipy requests matplotlib bpython ptpython"
				],
				"pattern": "^(.*)$"
			}
		},
		"tests": {
			"$id": "#/properties/tests",
			"type": "object",
			"title": "The Tests Schema",
			"required": [],
			"additionalProperties": {"$ref": "#/definitions/test" }
		}
	},
	"additionalProperties": false
}